CREATE TABLE `users` (
    `user_id`       bigint(5)    NOT NULL auto_increment,
    `device_id`     char(30)     default NULL,
    `tracked_from`  timestamp    NOT NULL default CURRENT_TIMESTAMP,
    `user_name`     varchar(100),
    PRIMARY KEY  (`user_id`)
) ENGINE=MyISAM AUTO_INCREMENT=1001 DEFAULT CHARSET=latin2;
